{
  "name": "@shopify/app-bridge-core",
  "version": "1.5.0",
  "types": "index.d.ts",
  "main": "index.js",
  "module": "./esm/index.js",
  "files": [
    "/esm/",
    "/actions/",
    "/client/",
    "/util/",
    "/validate/",
    "/index.d.ts",
    "/index.js",
    "/MessageTransport.d.ts",
    "/MessageTransport.js"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./esm/index.js",
      "default": "./index.js"
    },
    "./*": {
      "types": "./*.d.ts",
      "import": "./esm/*.js",
      "default": "./*.js"
    },
    "./actions": {
      "types": "./actions/index.d.ts",
      "import": "./esm/actions/index.js",
      "default": "./actions/index.js"
    },
    "./actions/*": {
      "types": "./actions/*/index.d.ts",
      "import": "./esm/actions/*/index.js",
      "default": "./actions/*/index.js"
    },
    "./actions/ActionSet": {
      "types": "./actions/ActionSet.d.ts",
      "import": "./esm/actions/ActionSet.js",
      "default": "./actions/ActionSet.js"
    },
    "./actions/buttonHelper": {
      "types": "./actions/buttonHelper.d.ts",
      "import": "./esm/actions/buttonHelper.js",
      "default": "./actions/buttonHelper.js"
    },
    "./actions/constants": {
      "types": "./actions/constants.d.ts",
      "import": "./esm/actions/constants.js",
      "default": "./actions/constants.js"
    },
    "./actions/helper": {
      "types": "./actions/helper.d.ts",
      "import": "./esm/actions/helper.js",
      "default": "./actions/helper.js"
    },
    "./actions/merge": {
      "types": "./actions/merge.d.ts",
      "import": "./esm/actions/merge.js",
      "default": "./actions/merge.js"
    },
    "./actions/types": {
      "types": "./actions/types.d.ts",
      "import": "./esm/actions/types.js",
      "default": "./actions/types.js"
    },
    "./actions/uuid": {
      "types": "./actions/uuid.d.ts",
      "import": "./esm/actions/uuid.js",
      "default": "./actions/uuid.js"
    },
    "./actions/validator": {
      "types": "./actions/validator.d.ts",
      "import": "./esm/actions/validator.js",
      "default": "./actions/validator.js"
    },
    "./actions/Navigation/*": {
      "types": "./actions/Navigation/*/index.d.ts",
      "import": "./esm/actions/Navigation/*/index.js",
      "default": "./actions/Navigation/*/index.js"
    },
    "./actions/Link/*": {
      "types": "./actions/Link/*/index.d.ts",
      "import": "./esm/actions/Link/*/index.js",
      "default": "./actions/Link/*/index.js"
    },
    "./actions/Menu/*": {
      "types": "./actions/Menu/*/index.d.ts",
      "import": "./esm/actions/Menu/*/index.js",
      "default": "./actions/Menu/*/index.js"
    },
    "./actions/Modal/*": {
      "types": "./actions/Modal/*/index.d.ts",
      "import": "./esm/actions/Modal/*/index.js",
      "default": "./actions/Modal/*/index.js"
    },
    "./client": {
      "types": "./client/index.d.ts",
      "import": "./esm/client/index.js",
      "default": "./client/index.js"
    },
    "./client/*": {
      "types": "./client/*.d.ts",
      "import": "./esm/client/*.js",
      "default": "./client/*.js"
    },
    "./util/*": {
      "types": "./util/*.d.ts",
      "import": "./esm/util/*.js",
      "default": "./util/*.js"
    },
    "./validate/*": {
      "types": "./validate/*.d.ts",
      "import": "./esm/validate/*.js",
      "default": "./validate/*.js"
    },
    "./validate/actions": {
      "types": "./validate/actions/index.d.ts",
      "import": "./esm/validate/actions/index.js",
      "default": "./validate/actions/index.js"
    },
    "./validate/actions/*": {
      "types": "./validate/actions/*.d.ts",
      "import": "./esm/validate/actions/*.js",
      "default": "./validate/actions/*.js"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "repository": "git@github.com:Shopify/app-bridge.git",
  "homepage": "https://shopify.dev/tools/app-bridge",
  "author": "Shopify Inc.",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:tsc; rollup --config ./rollup.config.mjs",
    "build:tsc": "NODE_ENV=production tsc",
    "check": "tsc",
    "clean": "NODE_ENV=production tsc --build --clean; rm -rf ./esm",
    "size": "size-limit"
  },
  "sideEffects": false,
  "size-limit": [
    {
      "limit": "4.15 KB",
      "path": "index.js"
    }
  ]
}
