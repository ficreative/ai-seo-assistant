{
  "name": "ai-seo-assistant",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:web": "prisma generate && prisma db push && npx vite --config ./vite.config.js --host 127.0.0.1 --port ${PORT:-5173} --strictPort",
    "worker": "node app/worker/seo.worker.js",
    "dev": "node scripts/dev-with-worker.js",
    "dev:shopify": "shopify app dev",
    "build": "react-router build",
    "config:link": "shopify app config link",
    "generate": "shopify app generate",
    "deploy": "shopify app deploy",
    "config:use": "shopify app config use",
    "env": "shopify app env",
    "start": "node ./build/server/index.js",
    "docker-start": "npm run setup && npm run start",
    "setup": "prisma generate && prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "shopify": "shopify",
    "prisma": "prisma",
    "graphql-codegen": "graphql-codegen",
    "vite": "vite",
    "typecheck": "react-router typegen && tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@react-router/fs-routes": "^7.13.0",
    "@sentry/node": "^9.17.0",
    "@sentry/react": "^9.17.0",
    "@shopify/app-bridge": "^3.7.11",
    "@shopify/app-bridge-react": "^4.2.8",
    "@shopify/polaris": "^13.9.2",
    "@shopify/polaris-icons": "^9.3.1",
    "@shopify/shopify-api": "^12.3.0",
    "@shopify/shopify-app-react-router": "^1.1.0",
    "@shopify/shopify-app-session-storage-prisma": "^8.0.0",
    "bullmq": "^5.66.7",
    "ioredis": "^5.7.2",
    "isbot": "^5.1.31",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "7.13.0"
  },
  "devDependencies": {
    "@react-router/dev": "7.13.0",
    "prisma": "^6.19.0",
    "vite-tsconfig-paths": "^6.1.1"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}
